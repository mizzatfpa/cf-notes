{
    "manifest_version": 3,
    "name": "CF Notes",
    "version": "1.0",
    "description": "Track Codeforces problems and your solution notes.",
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "https://codeforces.com/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}